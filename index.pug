doctype html
html
  head
    meta(charset='utf-8' name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1")
    meta(http-equiv="refresh" content="900")
    link(href="//cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet")
    link(href="//cdn.bootcss.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet")
    link(href="/default.css" rel="stylesheet")
    title!=title  
    style(type="text/css").
      h1 {color:red;}
      p {color:blue;}
      #home {margin: 20px 0px}
  body
    center 
        h1 ANN 更新
        - var update = message.pop();
        p 每小时抓取，最近更新：#{update.updateTime}
    div(id="home")
      p #POW ANN (Total: #{message[0].powData.length})
      ul
        for item in message[0].powData      
            li
               a(target= "_blank" href=item.ann_href)       #{item.ann_topicID}  :   #{item.ann_title}
      p #MN ANN (Total: #{message[1].mnData.length})
      ul
        for item in message[1].mnData      
            li
               a(target= "_blank" href=item.ann_href)       #{item.ann_topicID}  :   #{item.ann_title}
      p #POS ANN (Total:  #{message[2].posData.length})
      ul
        for item in message[2].posData      
            li
               a(target= "_blank" href=item.ann_href)       #{item.ann_topicID}  :   #{item.ann_title}              
  footer
        div(class="text-muted")
           span 出品: 
             a(href="https://www.cnpool.vip")
                span  空气池 
           span   QQ群:686394624
